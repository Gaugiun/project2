<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.rest.common.persistence.dao.BaseHallTypeListMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.stylefeng.guns.rest.bean.BaseHallTypeListVO">
		<id column="UUID" property="halltypeId" />
		<result column="show_Name" property="halltypeName" />
	</resultMap>
	<sql id="HallTypeList">
		UUID , show_Name
	</sql>

	<select id="select" resultMap="BaseResultMap">
		select <include refid="HallTypeList"/> from mtime_hall_dict_t
	</select>

</mapper>
